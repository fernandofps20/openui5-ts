{
	"_version": "1.12.0",

	"sap.app": {
		"id": "com.myorg.myapp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{app_title}}",
		"description": "{{app_description}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "com.myorg.myapp.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.90.1",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {}
			}
		},
		"handleValidation": true,
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"config": {
			"invoiceRemote": "https://services.odata.org/V2/Northwind/Northwind.svc/"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.myorg.myapp.i18n.i18n"
				}
			}
		},
		"resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
		"routing": {
			"config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.myorg.myapp.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
			"routes": [
				{
					"name": "RouteApp",
					"pattern": "",
					"target": ["TargetApp"]
				},
				{
					"name": "RouteNextView",
					"pattern": "NextView",
					"target": ["TargetNextView"]
				}
			],
            "targets": {
				"TargetApp": {
					"viewType": "XML",
					"transition": "slide",
					"viewId": "App",
					"viewName": "App"
				},
				"TargetNextView": {
					"viewType": "XML",
					"transition": "slide",
					"viewId": "NextView",
					"viewName": "NextView"
				}
            }
		}
	}
}